@{
    ViewBag.Title = "个人信息";
}

<ul class=" list-group">
    @foreach(var claim in User.Claims)
    {
        <li class="list-group-item">
            <span class=" list-group-item-heading">@claim.Type</span>
            <span class="list-group-item-text">@claim.Value</span>
        </li>

    }
</ul>